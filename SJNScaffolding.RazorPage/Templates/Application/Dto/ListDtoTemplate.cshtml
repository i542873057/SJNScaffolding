@model SJNScaffolding.Models.TemplateModels.ViewFileModel
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using Abp.Application.Services.Dto;
using Abp.AutoMapper;

namespace @(Model.ProjectName).Domain.Plat.@(Model.TableName)s.Dto
{
    [AutoMapFrom(typeof(@(Model.TableName)))]
    public class @(Model.TableName)ListDto : EntityDto<@(Model.IdType)>
    {
        @if(Model.ListColumnNames != null)
        {
            foreach (var item in Model.ListColumnNames)
            {
@("     ///" )<summary>
@Html.Raw("     ///" + item.Comments) 
@("     ///" )</summary>
@Html.Raw("     public " + item.AttrType + " " + item.ColumnName + "{get;set;} \r\n")
            }
        }
    }
}
